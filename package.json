{
  "name": "leonardo-ui",
  "version": "1.2.0",
  "description": "UI Library for Qlik Sense web projects",
  "author": "QlikTech International AB",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:qlik-oss/leonardo-ui.git"
  },
  "files": [
    "/dist"
  ],
  "main": "dist/leonardo-ui.min.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "build": "rollup -c && rollup -c --environment BUILD:production",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test aw-test-runner ./test/unit --compilers js:babel-register",
    "test:ci": "cross-env NODE_ENV=test node ./tools/build-fixtures.js && webdriver-manager update --standalone false && aw-webdriver-test-runner ./test/circle.js --specs ./test/component/*.spec.js --compilers js:babel-register",
    "start": "rollup -c ./rollup.config.docs.js --watch",
    "coverage": "cross-env NODE_ENV=test aw-test-coverage -- test/unit --compilers js:babel-register",
    "docs": "node ./tools/build-docs.js"
  },
  "devDependencies": {
    "after-work.js": "^2.2.0",
    "autoprefixer": "^6.2.3",
    "babel-core": "^6.10.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.7.2",
    "browser-sync": "^2.18.2",
    "cross-env": "^5.0.1",
    "eslint": "^3.8.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "extend": "^3.0.0",
    "glob": "^7.0.5",
    "handlebars": "^4.0.5",
    "less": "^2.5.0",
    "protractor": "^5.1.1",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-browsersync": "^0.2.4",
    "rollup-plugin-less": "^0.1.3",
    "rollup-plugin-license": "^0.5.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-postcss": "^0.5.4",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-serve": "^0.4.1",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.0.0"
  }
}
